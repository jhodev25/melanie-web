sers/jordanho/Develop/melanie-web/src/vite-env.d.ts",[],[],"/Users/jordanho/Develop/melanie-web/src/containers/providers.tsx",[],[],"/Users/jordanho/Develop/melanie-web/src/containers/route-container.tsx",[],[],"/Users/jordanho/Develop/melanie-web/src/pages/home.tsx",[],[],"/Users/jordanho/Develop/melanie-web/src/pages/index.tsx",[],[],"/Users/jordanho/Develop/melanie-web/src/redux/store.ts",[],[],"/Users/jordanho/Develop/melanie-web/src/components/index.ts",[],[],"/Users/jordanho/Develop/melanie-web/src/@types/assets/index.d.ts",[],[],"/Users/jordanho/Develop/melanie-web/src/components/lib/menu/index.ts",[],[],"/Users/jordanho/Develop/melanie-web/src/components/lib/menu/menu.tsx",["169","170","171"],[],"import MenuIcon from '@mui/icons-material/Menu';\nimport { IconButton, Link, Menu, MenuItem } from '@mui/material';\nimport { useState } from 'react';\n\nconst options = ['Home', 'Projects', 'About', 'Contact Me'];\n\nconst ITEM_HEIGHT = 48;\n\nexport const MenuButton = () => {\n  const [anchorEl, setAnchorEl] = useState(null);\n  const open = Boolean(anchorEl);\n  const handleClick = (event: any) => {\n    setAnchorEl(event.currentTarget);\n  };\n  const handleClose = (option: any) => {\n    if (option === 'Home') {\n      window.location.assign('/');\n    } else if (option === 'Contact Me') {\n      window.location.assign('/contact');\n    } else window.location.assign(`/${option}`);\n    setAnchorEl(null);\n  };\n\n  return (\n    <div>\n      <IconButton\n        aria-label=\"more\"\n        id=\"long-button\"\n        aria-controls={open ? 'long-menu' : undefined}\n        aria-expanded={open ? 'true' : undefined}\n        aria-haspopup=\"true\"\n        onClick={handleClick}\n      >\n        <MenuIcon />\n      </IconButton>\n      <Menu\n        id=\"menu\"\n        MenuListProps={{\n          'aria-labelledby': 'long-button',\n        }}\n        anchorEl={anchorEl}\n        open={open}\n        onClose={handleClose}\n        PaperProps={{\n          style: {\n            maxHeight: ITEM_HEIGHT * 4.5,\n            width: '20ch',\n          },\n        }}\n      >\n        {options.map((option) => (\n          <MenuItem key={option} selected={option === 'Pyxis'} onClick={() => handleClose(option)}>\n            {option}\n          </MenuItem>\n        ))}\n      </Menu>\n    </div>\n  );\n};\n","/Users/jordanho/Develop/melanie-web/src/components/lib/projectCard/index.tsx",[],[],"/Users/jordanho/Develop/melanie-web/src/components/lib/projectCard/projectCard.tsx",[],[],"/Users/jordanho/Develop/melanie-web/src/pages/projects/projects.tsx",[],[],"/Users/jordanho/Develop/melanie-web/src/components/lib/tabs/index.tsx",[],[],"/Users/jordanho/Develop/melanie-web/src/pages/about/about.tsx",["172","173"],[],"import { melHome } from 'assets/images';\n\nexport const About = () => {\n  return (\n    <div className=\"columns-2\">\n      <img src={melHome} alt=\"cover\" />\n      <h1>ABOUT</h1>\n      <h3>\n        From my earliest days, I embraced the label of an introvert. However, as life unfolded, I discovered that\n        pushing my boundaries and embracing independence could be a thrilling adventure. It was in moments of\n        vulnerability and openness that I truly connected with others and grew.\n      </h3>\n      <h3>\n        My journey took me far from my hometown to embark on a college adventure in a different state, and later, I\n        crossed oceans for a transformative study abroad experience. These calculated risks not only expanded my\n        horizons but also helped me build confidence and adaptability.\n      </h3>\n      <h3>\n        In Portland, I delved into the worlds of marketing and fine arts, gaining valuable experience in both non-profit\n        and luxury automotive sectors. Collaborating with numerous vendors and creative partners, I developed a deep\n        passion for curating and coordinating unforgettable experiences.\n      </h3>\n      <h3>\n        Now, I've returned to my roots in San Francisco, in search of opportunities that resonate with my sense of home\n        and belonging. I'm thrilled to join an industry where I can wholeheartedly dedicate myself to creating\n        exceptional shared experiences between brands and communities.\n      </h3>\n      <h3>\n        In essence, my goal is to gather professional and personal experiences that fill my life with gratitude and joy.\n        I find immense value in the relationships I form and the communities that teach me about myself, as I continue\n        to explore and grow in this vast world.\n      </h3>\n      <h3>\n        In my free time, you can find me taking the streetcar to my favorite corners of San Francisco, walking through\n        the lawns of Golden Gate Park, taking a light jog to Ocean Beach, or volunteering at the Asian Art Museum.\n      </h3>\n    </div>\n  );\n};\n","/Users/jordanho/Develop/melanie-web/src/pages/about/index.tsx",[],[],"/Users/jordanho/Develop/melanie-web/src/pages/contact/contact.tsx",[],[],"/Users/jordanho/Develop/melanie-web/src/pages/contact/index.tsx",[],[],"/Users/jordanho/Develop/melanie-web/src/pages/eventCoord/eventCoord.tsx",["174"],[],"import { Button } from '@mui/material';\n\nimport { eventsFifteen, eventsFive, eventsFourteen, eventsMel, eventsNine, melHome } from 'assets/images';\n\nexport const EventCoordination = () => {\n  return (\n    <div className=\"w-auto h-auto\">\n      <h1>Event Coordination</h1>\n      <div className=\"columns-2\">\n        <h2>LEAD EVENT COORDINATOR</h2>\n        <h3>\n          I spearheaded event planning initiatives in my role post-COVID-19 in 2021-2022. My responsibilities\n          encompassed a wide range of events, with attendees ranging from 15 to 200, spanning both large-scale and\n          intimate gatherings, hosted both at our dealership and various off-site locations.\n        </h3>\n        <h3>\n          I was entrusted with significant autonomy, empowering me to drive creative direction and make strategic\n          decisions in collaboration with our Marketing Director.\n        </h3>\n        <img src={eventsMel} alt=\"mel\" />\n      </div>\n      <div className=\"h-auto\">\n        <h2 className=\"text-center\">RESPONSIBILITIES</h2>\n        <div className=\"columns-2\">\n          <div className=\"m-auto\">\n            <img src={eventsNine} alt=\"event space\" />\n          </div>\n          <div className=\"h-fit bg-cyan-300\">\n            <h3>\n              Proficiently designed presentation slides to propose event concepts, outline event details, and create\n              floorplans, facilitating efficient planning and visualization.\n            </h3>\n            <Button>View Sample Deck</Button>\n            {/* todo: add action to this button */}\n            <h3>\n              Acted as the central point of contact for partnership activities, liaising effectively with vendors and\n              third-party collaborators throughout the event lifecycle, including planning, execution, and\n              post-activation, demonstrating strong vendor and stakeholder management abilities.\n            </h3>\n            <br />\n            <h3>\n              Maintained meticulous and organized records, including invoices, budgeting documents, guest lists, RSVP\n              information, and key performance metrics, ensuring efficient event coordination and financial oversight.\n            </h3>\n            <br />\n            <h3>\n              Expertly managed event timelines in collaboration with stakeholders, guaranteeing punctuality for\n              important milestones, such as gifts, reservations, and deadlines, showcasing strong project management.\n            </h3>\n          </div>\n        </div>\n      </div>\n      <br />\n      <div className=\"columns-2\">\n        <h3>\n          Developed comprehensive shot lists for photo and video content collection, optimizing media capture during\n          events for effective documentation and future promotional material.\n        </h3>\n        <h3>\n          Provided hands-on, on-site supervision during event setup and tear-down, contributing to the seamless\n          execution of events while ensuring the accordance of facility guidelines and guest safety.\n        </h3>\n        <img src={eventsFifteen} alt=\"porsche store\" />\n      </div>\n      <div className=\"columns-3\">\n        <img src={eventsFive} alt=\"cover\" />\n        <img src={melHome} alt=\"cover\" />\n        <img src={eventsFourteen} alt=\"cover\" />\n      </div>\n      <div className=\"content-center bg-orange-400\">\n        <h3>\n          During her time with Porsche Beaverton, I had the pleasure of partnering with Melanie in my role as Sales &\n          Hospitality Manager at Domaine Divio in Oregon’s Willamette Valley. Her professional, responsive communication\n          and thoughtful approach to reaching consumers made our partnership efficient and effective.\n        </h3>\n        <h3>\n          She has an energizing positive attitude, and was attentive and organized when planning events representing our\n          brands. I’m glad to have been able to work with her. If you are looking for someone who is capable and\n          marketing focused to represent your company, Melanie is your person.\n        </h3>\n        <h2>Matthew Melito</h2>\n        <h3>Sales & Hospitality Coordinator, Domaine Divio</h3>\n      </div>\n      <div>\n        <h1>CHRONOLOGY OF EVENTS</h1>\n        <div className=\"columns-2\">\n          <div>\n            <h3>Cars and Coffee (5 events per Summer)</h3>\n            <h3>Porsche Cup Golf Tournament</h3>\n            <h3>Porsche Taycan Launch</h3>\n            <h3>Domaine Serene Summer Car Classic</h3>\n            <h3>Portland Charity Toy Drive and Car Meet</h3>\n            <h3>Avants Portland Magazine Women's Day</h3>\n            <h3>Sunset Imports Company Party</h3>\n            <h3>Porsche Cup Golf Tournament</h3>\n            <h3>Porsche + Pinot Client Application</h3>\n            <h3>NW Passage Club Dinner</h3>\n            <h3>VIP Client Coctail Hour</h3>\n            <h3>Domaine Serene Summer Car Classic</h3>\n          </div>\n          <div className=\"text-end\">\n            <h3> Summer 2021/22</h3>\n            <h3> June 2021</h3>\n            <h3> July 2021</h3>\n            <h3> August 2021</h3>\n            <h3> December 2021</h3>\n            <h3> March 2022</h3>\n            <h3> May 2022</h3>\n            <h3> May 2022</h3>\n            <h3> May 2022</h3>\n            <h3> June 2022</h3>\n            <h3> June 2022</h3>\n            <h3> August 2022</h3>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n","/Users/jordanho/Develop/melanie-web/src/pages/eventCoord/index.tsx",[],[],"/Users/jordanho/Develop/melanie-web/src/pages/influencerMarketing/index.tsx",[],[],"/Users/jordanho/Develop/melanie-web/src/pages/influencerMarketing/influencerMarketing.tsx",["175","176","177","178"],[],"import { influencerFour, influencerOne, influencerSeven, influencerThree, melHome } from 'assets/images';\n\nexport const InfluencerMarketing = () => {\n  return (\n    <div className=\"w-auto h-auto\">\n      <div className=\"columns-2 bg-red-300\">\n        <div>\n          <img src={influencerOne} alt=\"cover\" />\n          <img src={influencerSeven} alt=\"cover\" />\n        </div>\n        <div>\n          <h1>INFLUENCER MARKETING</h1>\n          <h2>DESCRIPTION</h2>\n          <h3>\n            Pioneered the launch of the company's first influencer partnership campaigns, orchestrating impactful\n            marketing campaigns with two prominent local social media influencers.\n          </h3>\n          <h3>\n            These campaigns featured collaborative content posts on Instagram and introduced a successful giveaway\n            component.\n          </h3>\n        </div>\n      </div>\n      <div className=\"text-center\">\n        <h2>RESPONSIBILITIES</h2>\n        <h3>\n          Conducted thorough research to identify influencers who aligned with brand values and catered to the newly\n          acquired target markets.\n        </h3>\n        <h3>\n          Crafted and revised proposals, ensuring a mutual understanding of deliverables, campaign terms, and financial\n          compensation between influencers and the brand.\n        </h3>\n        <h3>\n          Created comprehensive copy and social post guidelines to ensure alignment between influencers and the brand,\n          with all content coordinated and approved via a shared calendar.\n        </h3>\n        <h3>\n          Managed invoices, expenses, and key performance indicators, tracking metrics like impressions, average likes,\n          giveaway entries, and follower growth.\n        </h3>\n        <h3>\n          Fostered relationships with local businesses and vendors to coordinate giveaway prizes, establishing trust and\n          goodwill with valued brands.\n        </h3>\n        <h3>\n          Generated a detailed report summarizing the end-to-end planning and execution of the campaign, including a\n          project summary, KPI report, and post-activation summary.\n        </h3>\n      </div>\n      <br />\n      <div className=\"columns-2\">\n        <div>\n          <h3>\n            Grew the brand's social following by 500+ during both campaigns, initiating a steady overall increase in the\n            following months.\n          </h3>\n          <h3>\n            Increased engagement and impression rates, enhancing the brand's online presence and interaction with the\n            audience.\n          </h3>\n          <h3>\n            Established long-lasting partnerships with local businesses, some of which evolved into valuable corporate\n            collaborations.\n          </h3>\n          <h3>\n            Recognized by Porsche North America for creating the best post- activation summary and overall comprehensive\n            report, underscoring the high quality and effectiveness of the campaign.\n          </h3>\n        </div>\n        <img src={influencerFour} alt=\"cover\" />\n      </div>\n      <div className=\"columns-2\">\n        <img src={influencerSeven} alt=\"cover\" />\n        <img src={influencerThree} alt=\"cover\" />\n      </div>\n    </div>\n  );\n};\n","/Users/jordanho/Develop/melanie-web/src/pages/marketingMerchandising/index.tsx",[],[],"/Users/jordanho/Develop/melanie-web/src/pages/marketingMerchandising/marketingMerchandising.tsx",["179","180"],[],"import { internship, melHome } from 'assets/images';\n\nexport const MarketingMerchandising = () => {\n  return (\n    <div>\n      <h1>MARKETING + MERCHANDISING INTERNSHIP</h1>\n      <div className=\"columns-2\">\n        <img src={internship} alt=\"cover\" />\n        <h2>DESCRIPTION</h2>\n        <h3>\n          During the summer of 2019, I had the privilege of interning at Chamber Music Northwest, a non- profit musical\n          performance organization.\n        </h3>\n        <h3>\n          In my role as the Marketing and Merchandising Intern, I took charge of merchandising and sales. Additionally,\n          I had the opportunity to observe and learn various marketing aspects within the unique context of a non-profit\n          organization.\n        </h3>\n      </div>\n      <div className=\"text-center\">\n        <h2>RESPONSIBILITIES</h2>\n        <h3>\n          Researched market trends and the target audience to select merchandise items in alignment with the festival's\n          brand identity.\n        </h3>\n        <h3>Managed the budget for merchandise development, ensuring profitability and quality.</h3>\n        <h3>\n          Established and maintained relationships with multiple vendors, overseeing timelines and price negotiations.\n        </h3>\n        <h3>\n          Created visually appealing table displays at nightly concerts, adjusting merchandise arrangements as needed.\n        </h3>\